    "@jest/globals": "^29.7.0",
    "@types/node": "^22.0.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.3",
    "typescript": "^5.8.3"
